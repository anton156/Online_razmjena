@{
    ViewData["Title"] = "Sličice";
}
@model IEnumerable<SliciceModel>

<div class="container">
    <h3 class="display-4">Sličice</h3>
    <div class="row">

        @foreach (var slicica in Model)
        {
            <div class="col-md-4">
                <partial name="_sliciceThumbnail" model="@slicica" />
            </div>
        }

    </div>
</div>
